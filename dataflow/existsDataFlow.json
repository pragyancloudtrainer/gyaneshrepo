{
	"name": "existsDataFlow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "srcEmpTbl",
						"type": "DatasetReference"
					},
					"name": "srcReadEmp"
				},
				{
					"linkedService": {
						"referenceName": "lsSQLDB",
						"type": "LinkedServiceReference"
					},
					"name": "srcReadDept"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          eid as integer,",
				"          deptid as integer,",
				"          ename as string,",
				"          eage as integer,",
				"          esalary as double",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> srcReadEmp",
				"source(output(",
				"          deptid as integer,",
				"          deptname as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table',",
				"     store: 'sqlserver',",
				"     schemaName: 'dbo',",
				"     tableName: 'Department',",
				"     isolationLevel: 'READ_UNCOMMITTED') ~> srcReadDept"
			]
		}
	}
}